{
  "build": {
    "development:simulator": {
      "distribution": "internal",
      "developmentClient": true,
      "channel": "development",
      "env": {
        "APP_ENV": "development"
      },
      "ios": {
        "simulator": true,
        "image": "latest"
      }
    },
    "development": {
      "distribution": "internal",
      "channel": "development",
      "env": {
        "APP_ENV": "development"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "image": "latest"
      }
    },
    "preview": {
      "distribution": "store",
      "channel": "preview",
      "env": {
        "TAMAGUI_TARGET": "native",
        "APP_ENV": "preview"
      },
      "ios": {
        "image": "latest"
      },
      "autoIncrement": true
    },
    "production": {
      "env": {
        "TAMAGUI_TARGET": "native"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {},
    "preview": {}
  },
  "cli": {
    "version": ">= 7.0.0"
  }
}
