{
  "expo": {
    "name": "تكلفة",
    "slug": "taklifa",
    "scheme": "taklifa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#333333"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.taklifa",
      "buildNumber": "1",
      "infoPlist": {
        "CFBundleDevelopmentRegion": "ar",
        "ExpoLocalization_supportsRTL": true,
        "CFBundleLocalizations": [
          "ar",
          "en"
        ],
        "LSApplicationQueriesSchemes": [
          "comgooglemaps",
          "citymapper",
          "uber",
          "lyft",
          "transit",
          "truckmap",
          "waze",
          "yandexnavi",
          "moovit",
          "yandextaxi",
          "yandexmaps",
          "kakaomap",
          "szn-mapy",
          "mapsme",
          "osmandmaps",
          "gett",
          "nmap",
          "dgis",
          "lftgpas",
          "comgooglemaps",
          "citymapper",
          "uber",
          "lyft",
          "transit",
          "truckmap",
          "waze",
          "yandexnavi",
          "moovit",
          "yandextaxi",
          "yandexmaps",
          "kakaomap",
          "szn-mapy",
          "mapsme",
          "osmandmaps",
          "gett",
          "nmap",
          "dgis",
          "lftgpas"
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:taklifa.app",
        "applinks:taklifa.app.link"
      ]
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FECA16"
      },
      "googleServicesFile": "./google-services.json",
      "package": "app.taklifa",
      "versionCode": 1,
      "permissions": [
        "android.permission.NOTIFICATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleSignIn": {
          "apiKey": "AIzaSyCivYWxpMBFI59BYFXK7c9hBHrgn3GHM-Q"
        },
        "googleMaps": {
          "apiKey": "AIzaSyBw3sZh4uFyLbi9sKTzKYn3BqIS_b-vGeA"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "taklifa.app",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "taklifa.app.link",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-updates",
      "expo-localization",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#FECA16",
          "defaultChannel": "default"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      "expo-apple-authentication",
      "expo-router",
      "expo-build-properties",
      "sentry-expo",
      "react-native-map-link"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "dce64616-882d-43d4-ac84-c3fa9e429d2a"
      }
    },
    "runtimeVersion": "1.0.0",
    "owner": "zixdev"
  }
}
