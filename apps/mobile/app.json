{
  "expo": {
    "name": "سواعد",
    "slug": "sawaeed",
    "jsEngine": "hermes",
    "scheme": "sawaeed",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#333333"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.sawaeed",
      "buildNumber": "13",
      "infoPlist": {
        "CFBundleDevelopmentRegion": "ar",
        "ExpoLocalization_supportsRTL": true,
        "CFBundleLocalizations": [
          "ar",
          "en",
          "ur"
        ],
        "LSApplicationQueriesSchemes": [
          "comgooglemaps",
          "citymapper",
          "uber",
          "lyft",
          "transit",
          "truckmap",
          "waze",
          "yandexnavi",
          "moovit",
          "yandextaxi",
          "yandexmaps",
          "kakaomap",
          "szn-mapy",
          "mapsme",
          "osmandmaps",
          "gett",
          "nmap",
          "dgis",
          "lftgpas"
        ]
      },
      "associatedDomains": [
        "applinks:sawaeed.app",
        "applinks:staging.sawaeed.app"
      ]
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FECA16"
      },
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "package": "app.sawaeed",
      "versionCode": 12,
      "permissions": [
        "NOTIFICATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.SCHEDULE_EXACT_ALARM"
      ],
      "config": {
        "googleSignIn": {
          "apiKey": "AIzaSyCivYWxpMBFI59BYFXK7c9hBHrgn3GHM-Q"
        },
        "googleMaps": {
          "apiKey": "AIzaSyBw3sZh4uFyLbi9sKTzKYn3BqIS_b-vGeA"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-localization",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#FECA16",
          "defaultChannel": "default"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      "expo-apple-authentication",
      "expo-router",
      "expo-build-properties",
      "sentry-expo",
      "react-native-map-link"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e42af032-da25-4337-9836-69ea04279e11"
      }
    },
    "runtimeVersion": "1.0.0",
    "owner": "zixdev",
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "zix-dev",
            "project": "sawaeed"
          }
        }
      ]
    }
  }
}
